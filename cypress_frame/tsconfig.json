{
  "compilerOptions": {
    "target": "es2015",
    "lib": ["es6", "dom"],
    "module": "commonjs",
    "skipLibCheck": true,
    "strict": true,
    "noImplicitAny": false,
    "types": [
      "cypress", "node"
    ],
    "outDir": "dist",
  },
  "include": [
    "cypress/**/*.ts",
    "node_modules/cypress"
  ],
}
