{
  "name": "cypress-test-frame",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test_spec": "./node_modules/.bin/cypress run --spec='cypress/**/custom.commands.spec.ts'",
    "test": "./node_modules/.bin/cypress run --config-file=cypress.json --headed --browser chrome",
    "cy:run:allure": "cypress run --env allure=true,TEST_ENV=AQA",
    "open": "./node_modules/.bin/cypress open --env config=dev",
    "cy:run:dev": "cypress run --env config=dev",
    "test:with:file": "node ./run.tests.js",
    "lint": "eslint . --ext .js,.ts && tsc",
    "prepare": "cd .. && husky install cypress_frame/.husky",
    "merge-junit-results": "npx junit-merge -d cypress/results/junit/ -o cypress/results/junit/results.xml",
    "delete-junit-results": "rm -rf cypress/results/*",
    "delete-junit-report": "rm -rf cypress/results/junit/results.xml",
    "merge-mochawesome-results": "npx mochawesome-merge cypress/results/mochawesome/*.json > cypress/results/mochawesome-results.json && npx marge cypress/results/mochawesome-results.json",
    "delete-mochawesome-report": "rm -rf mochawesome-report/*",
    "start-json-server": "json-server --watch db.json",
    "allure:generate": "node ./cypress/support/reporter/allureReportGenerator.js",
    "allure:open": "node ./cypress/support/reporter/allureOpen.js",
    "allure:report": "npm run allure:generate && npm run allure:open",
    "allure:merge": "node ./cypress/support/reporter/mergeAllureResults.js",
    "allure:sendResult": "node ./cypress/support/reporter/zipAndSendResult.js",
    "cleanup": "node ./cypress/support/reporter/clean.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "allure-commandline": "^2.13.8",
    "cypress": "8.4.1",
    "cypress-file-upload": "5.0.8",
    "form-data": "4.0.0",
    "fs-extra": "10.0.0",
    "node-fetch": "^2.1.2",
    "typescript": "4.4.3",
    "zip-a-folder": "1.1.0"
  },
  "devDependencies": {
    "@shelex/cypress-allure-plugin": "^2.19.5",
    "@types/cypress": "1.1.3",
    "@types/node": "14.17.0",
    "@typescript-eslint/eslint-plugin": "4.31.2",
    "@typescript-eslint/parser": "4.31.2",
    "cypress-multi-reporters": "1.5.0",
    "eslint": "7.32.0",
    "eslint-config-google": "0.14.0",
    "eslint-plugin-cypress": "2.12.1",
    "husky": "7.0.2",
    "json-server": "0.17.0",
    "mocha-junit-reporter": "2.0.0",
    "mochawesome": "6.3.1",
    "mochawesome-merge": "4.2.0",
    "mochawesome-report-generator": "5.2.0"
  }
}
